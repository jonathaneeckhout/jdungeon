config_version=5

[application]

config/name="Jdungeon"
run/main_scene="res://scenes/root/Root.tscn"
config/features=PackedStringArray("4.1", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GodotLogger="*res://addons/logger/logger.gd"
J="*res://scripts/singletons/jdungeon.gd"
JUI="*res://scripts/singletons/jui.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"

[debug]

gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="Jdungeon"

